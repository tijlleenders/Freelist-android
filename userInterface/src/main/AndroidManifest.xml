<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="nl.freelist.freelist">

  <application
    android:allowBackup="true"
    android:fullBackupContent="@xml/backup_descriptor"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
      android:name="nl.freelist.activities.NavigateFreelistActivity"
      android:launchMode="singleInstance">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
      android:name="nl.freelist.activities.AddEditEntryActivity"
      android:parentActivityName="nl.freelist.activities.NavigateFreelistActivity">
    </activity>
    <activity
      android:name="nl.freelist.activities.SettingsActivity"
      android:parentActivityName="nl.freelist.activities.NavigateFreelistActivity">
    </activity>
    <activity
      android:name="nl.freelist.activities.ChooseEntryActivity"
      android:parentActivityName="nl.freelist.activities.AddEditEntryActivity">
    </activity>
    <activity
      android:name="nl.freelist.activities.ChooseCalendarOptionActivity"
      android:parentActivityName="nl.freelist.activities.AddEditEntryActivity"></activity>
    <activity
      android:name="nl.freelist.activities.CalendarActivity"
      android:launchMode="singleInstance"></activity>
    <activity
      android:name="nl.freelist.activities.SearchActivity"
      android:launchMode="singleInstance">
    </activity>
  </application>

</manifest>